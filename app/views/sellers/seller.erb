<a href="/sellers/<%= seller.id %>">
  <div class="medium-4 columns">
    <div class="profile-card">
      <img src="<%= seller.image %>">
      <div class="profile-info">
        <h4 class="subheader"><%= seller.name %></h4>
        <h5>A little more about me..</h5>
        <p>
          <%= seller.blurb %>
        </p>
      </div>
        <p style="text-align:center"><small>Created: <%= seller.created_at %> </small><br>
         <small>Updated: <%= seller.updated_at %></small></p> 
    </div>
  </div>
</a>
